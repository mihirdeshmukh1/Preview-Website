{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Start Development Server",
            "type": "shell",
            "command": "cd /workspaces/Preview-Website/raaghu-react/raaghu-pages && npm run dev",
            "isBackground": true,
            "problemMatcher": {
                "owner": "typescript",
                "pattern": {
                    "regexp": "."
                },
                "background": {
                    "beginsPattern": ".*",
                    "endsPattern": "."
                }
            },
            "runOptions": {
                "runOn": "folderOpen"
            }
        }
    ]
}