{
    "name": "Raaghu React Preview",
    "image": "mcr.microsoft.com/devcontainers/javascript-node:18",
    "customizations": {
      "vscode": {
        "extensions": [
          "dbaeumer.vscode-eslint",
          "esbenp.prettier-vscode",
          "ms-vscode.vscode-typescript-next"
        ]
      }
    },
    "forwardPorts": [5173],
    "postCreateCommand": "cd /workspaces/Preview-Website && npm run install-all",
    "postStartCommand": "cd /workspaces/Preview-Website/raaghu-react/raaghu-pages && npm run dev",
    "portsAttributes": {
      "5173": {
        "label": "Application",
        "onAutoForward": "openBrowser"
      }
    }
  }